<!-- app/views/likes/_form.html.erb -->
<% if @likeable %>
  <%= form_with(model: @likeable.likes.build(user: current_user, like_status: :like), local: true) do |form| %>
    <%= form.submit 'Like' %>
  <% end %>

  <%= form_with(model: @likeable.likes.build(user: current_user, like_status: :dislike), local: true) do |form| %>
    <%= form.submit 'Dislike' %>
  <% end %>
<% end %>
